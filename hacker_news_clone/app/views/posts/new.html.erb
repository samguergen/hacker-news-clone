<% if current_user%>
  <%= form_for(@new_post) do |f|%>
  <fieldset>
    <legend> Create New Post </legend>
    <%= f.label :title%>
    <%= f.text_area :title, placeholder: 'Title Me'%><br>

    <%= f.label :content%>
    <%= f.text_area :content, placeholder: 'Post something wonderful!'%>

    <%= f.submit%>
  </fieldset>
  <%end%>
<%else%>
  "HELLLLLO YOU ARE HERE NOW. MUST LOGIN NOWWwWWWWWWWWw"
<%end%>
